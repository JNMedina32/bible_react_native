{
  "expo": {
    "name": "bibleF",
    "slug": "bibleF",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sheep.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/sheep.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdf6e3"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sheep.png",
        "backgroundColor": "#fdf6e3"
      },
      "package": "com.sheep.bible"
    },
    "web": {
      "favicon": "./assets/images/sheep.png"
    },
    "plugins": [
      [
        "expo-asset",
        {
          "assets": ["./assets/bible.db"]
        }
      ]
    ]
  }
}
